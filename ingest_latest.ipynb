    "import PyPDF2\n",
    "import os\n",
    "\n",
    "def extract_text_from_pdfs_with_pypdf2(pdf_folder):\n",
    "    texts = []\n",
    "    for filename in os.listdir(pdf_folder):\n",
    "        if filename.endswith('.pdf'):\n",
    "            pdf_path = os.path.join(pdf_folder, filename)\n",
    "            with open(pdf_path, 'rb') as f:\n",
    "                pdf_reader = PyPDF2.PdfReader(f)\n",
    "                text = \"\"\n",
    "                for page_num in range(len(pdf_reader.pages)):\n",
    "                    page_obj = pdf_reader.pages[page_num]\n",
    "                    text += page_obj.extract_text()\n",
    "                texts.append(text)\n",
    "    return texts\n",
    "\n",
    "\n",
    "from openai import OpenAI\n",
    "from langchain.vectorstores import FAISS\n",
    "from langchain.document_loaders import PyPDFLoader, DirectoryLoader\n",
    "from langchain.text_splitter import RecursiveCharacterTextSplitter \n",
    "import numpy as np\n",
    "\n",
    "# Initialize OpenAI client\n",
    "client = OpenAI(api_key= os.getenv(\"OPENAI_API_KEY\")\n")\n",
    "\n",
    "def generate_embeddings(texts, model=\"text-embedding-ada-002\"):\n",
    "    embeddings = []\n",
    "    for text in texts:\n",
    "        if text.strip():\n",
    "            try:\n",
    "                # Attempt to create an embedding for the text\n",
    "                response = client.embeddings.create(input=text, model=model)\n",
    "                # Try accessing the embedding data\n",
    "                # Adjust this line based on the correct way to extract embedding from the response\n",
    "                embedding_data = response.to_dict()['data'][0]['embedding']\n",
    "                embeddings.append(embedding_data)\n",
    "            except AttributeError:\n",
    "                # If response object doesn't have .to_dict(), try another method or check documentation\n",
    "                print(\"Failed to extract embedding. Check the response structure and documentation.\")\n",
    "                continue\n",
    "            except KeyError:\n",
    "                # Handle cases where the expected keys aren't found in the response dictionary\n",
    "                print(\"Unexpected response format. Check the API documentation for updates.\")\n",
    "                continue\n",
    "        else:\n",
    "            print(\"Encountered empty text, skipping...\")\n",
    "            # Handle empty text as needed\n",
    "    return np.array(embeddings)\n",
    "\n",
    "\n",
    "import faiss\n",
    "\n",
    "def create_faiss_index(embeddings):\n",
    "    dimension = embeddings.shape[1]  # Dimensionality of the embeddings\n",
    "    index = faiss.IndexFlatL2(dimension)  # Using L2 distance for similarity\n",
    "    index.add(embeddings)  # Add embeddings to the index\n",
    "    return index\n",
    "\n",
    "def save_faiss_index(index, save_path='db_faiss'):\n",
    "    os.makedirs(save_path, exist_ok=True)\n",
    "    index_path = os.path.join(save_path, 'faiss_index.bin')\n",
    "    faiss.write_index(index, index_path)\n",
    "    print(\"Vector DB Stored Successfully\")\n",
    "\n",
    "def process_pdfs_and_create_faiss_db_with_pypdf2(pdf_folder):\n",
    "    # Extract text from all PDFs in the folder using PyPDF2\n",
    "    texts = extract_text_from_pdfs_with_pypdf2(pdf_folder)\n",
    "    \n",
    "    embeddings = generate_embeddings(texts)\n",
    "    \n",
    "    # Create a FAISS index and add the embeddings\n",
    "    faiss_index = create_faiss_index(embeddings)\n",
    "    \n",
    "    # Save the FAISS index to disk\n",
    "    save_faiss_index(faiss_index)\n",
    "\n",
    "# Example usage\n",
    "if __name__ == \"__main__\":\n",
    "    process_pdfs_and_create_faiss_db_with_pypdf2('./')\n",
    "\n",
    "\n"
   ]
